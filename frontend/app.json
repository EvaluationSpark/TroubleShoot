{
  "expo": {
    "name": "Fix Stuff",
    "slug": "fixintel-ai",
    "version": "1.0.7",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "fixintel",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "owner": "rentmouse",
    "extra": {
      "eas": {
        "projectId": "9d5fce54-6e1a-4587-ae9a-34b363c59406"
      }
    },
    "description": "AI-powered repair guide app that helps you fix broken items with step-by-step instructions, tutorial videos, and local repair shop recommendations.",
    "notification": {
      "icon": "./assets/images/icon.png",
      "color": "#00D9FF",
      "androidMode": "default",
      "androidCollapsedTitle": "#{unread_notifications} new repairs"
    },
    "splash": {
      "image": "./assets/images/splash-combined.png",
      "resizeMode": "cover",
      "backgroundColor": "#0a0a0a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fixintel.ai",
      "buildNumber": "1.0.7",
      "requireFullScreen": false,
      "infoPlist": {
        "NSCameraUsageDescription": "FixIntel AI needs camera access to analyze broken items for repair diagnostics.",
        "NSPhotoLibraryUsageDescription": "FixIntel AI needs photo library access to select images for repair analysis.",
        "NSLocationWhenInUseUsageDescription": "FixIntel AI needs your location to find local repair professionals near you.",
        "UIRequiresFullScreen": false,
        "UISupportedInterfaceOrientations~ipad": [
          "UIInterfaceOrientationPortrait",
          "UIInterfaceOrientationPortraitUpsideDown",
          "UIInterfaceOrientationLandscapeLeft",
          "UIInterfaceOrientationLandscapeRight"
        ]
      }
    },
    "androidStatusBar": {
      "backgroundColor": "#000000",
      "barStyle": "light-content"
    },
    "android": {
      "package": "com.fixintel.ai",
      "versionCode": 7,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-combined.png",
          "imageWidth": 400,
          "resizeMode": "cover",
          "backgroundColor": "#0a0a0a"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
